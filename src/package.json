{
  "name": "condo-convid19",
  "private": true,
  "version": "0.0.0",
  "repository": "git@github.com:Expo-Sales/condo-covid-19.git",
  "scripts": {
    "dev": "cross-env NODE_ENV=local serverless offline start --stage local --port 3000 --host 0.0.0.0",
    "debug": "cross-env NODE_ENV=local node ./node_modules/.bin/serverless offline start --stage local --port 3000 --host 0.0.0.0 --lazy --inspect=0.0.0.0",
    "pretest-integration": "NODE_ENV=test sequelize db:migrate",
    "test-integration": "cross-env NODE_ENV=test jest --verbose ./__tests__/integration --detectOpenHandles --forceExit --silent",
    "posttest-integration": "NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest-integration:watch": "NODE_ENV=test sequelize db:migrate",
    "test-integration:watch": "NODE_ENV=test jest -i --config ./jest.config.js --watchAll --verbose ./__tests__/integration",
    "posttest-integration:watch": "NODE_ENV=test sequelize db:migrate:undo:all",
    "test-e2e": "cross-env NODE_ENV=test jest --coverage=false --verbose ./__tests__/e2e",
    "test-e2e:watch": "cross-env NODE_ENV=test sequelize db:migrate:undo:all && sequelize db:migrate && jest -i --watchAll --coverage=false --verbose ./__tests__/e2e",
    "migrate": "sequelize db:migrate",
    "testall-up-graphql": "cross-env NODE_ENV=local serverless offline start --stage local --port 3000 --host 0.0.0.0 &",
    "testall": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test jest --verbose  ./__tests__/integration --forceExit",
    "run-migrations": "sequelize db:migrate"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.13",
    "@sendgrid/mail": "^6.5.0",
    "age-calculator": "^1.0.0",
    "apollo-server-lambda": "^2.9.8",
    "async-redis": "^1.1.7",
    "aws-sdk": "^2.574.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "bluebird": "^3.7.2",
    "dotenv": "^8.2.0",
    "download": "^7.1.0",
    "easygraphql-tester": "^5.1.6",
    "fb": "^2.0.0",
    "firebase-admin": "^8.9.2",
    "form-data": "^3.0.0",
    "fs": "^0.0.1-security",
    "generate-password": "^1.5.0",
    "graphql": "^14.5.8",
    "graphql-schema-modules": "^0.0.4",
    "image-to-base64": "^2.0.1",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "passport": "^0.4.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "redis": "^2.8.0",
    "sequelize": "^5.21.1",
    "system-sleep": "^1.3.6",
    "twilio": "^3.39.2",
    "util": "^0.12.1",
    "validator": "^12.1.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.23",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "aws-sdk-mock": "^4.5.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "buffer": "^5.4.3",
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "firebase-mock": "^2.3.2",
    "get-file-path": "^1.0.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "node-fetch": "^2.6.0",
    "sequelize-cli": "^5.5.1",
    "serverless": "^1.57.0",
    "serverless-domain-manager": "^3.3.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-localstack": "^0.4.20",
    "serverless-offline": "^5.12.0",
    "serverless-offline-scheduler": "^0.3.8",
    "serverless-plugin-warmup": "^4.7.1-rc.1",
    "serverless-webpack": "^5.3.1",
    "supertest": "^4.0.2",
